      "items": [
        {
          "type": "RowLayout",
          "items": [
            {
              "type": "PopupButton",
              "caption": "Versenden",
              "popup": {
                "caption": "Tagesbericht wirklich versenden?",
                "items": [
                  {
                    "type": "Button",
                    "caption": "Tagesbericht versenden",
                    "onClick": "BAT_TriggerDailyReport($id, false); echo 'Der Tagesbericht wurde versendet!';"
                  }
                ]
              }
            },
            {
              "type": "PopupButton",
              "caption": "Zurücksetzen",
              "popup": {
                "caption": "Tagesbericht wirklich zurücksetzen?",
                "items": [
                  {
                    "type": "Button",
                    "caption": "Tagesbericht zurücksetzen",
                    "onClick": "BAT_ResetDailyReportAttribute($id); echo 'Der Tagesbericht wurde zurückgesetzt!';"
                  }
                ]
              }
            },
            {
              "type": "Button",
              "caption": "Anzeigen",
              "onClick": "$data = BAT_GetDailyAttribute($id); echo $data;"
            }
          ]
        }
      ]
    },
    {
      "type": "ExpansionPanel",
      "caption": "Wochenbericht",
