              }
            },
            {
              "type": "PopupButton",
              "caption": "Zurücksetzen",
              "popup": {
                "caption": "Wochenbericht wirklich zurücksetzen?",
                "items": [
                  {
                    "type": "Button",
                    "caption": "Wochenbericht zurücksetzen",
                    "onClick": "BAT_ResetWeeklyReportAttribute($id); echo 'Der Tagesbericht wurde zurückgesetzt!';"
                  }
                ]
              }
            },
            {
              "type": "Button",
              "caption": "Anzeigen",
              "onClick": "$data = BAT_GetWeeklyAttribute($id); echo $data;"
            }
          ]
